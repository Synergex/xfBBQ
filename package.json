{
  "name": "xfbbq",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.3.1",
    "bootswatch": "^4.3.1",
    "eslint-plugin-react": "^7.16.0",
    "final-form": "^4.18.6",
    "jquery": "^3.4.1",
    "json-server": "^0.15.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-final-form": "^6.3.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-toastify": "^5.4.1",
    "redux": "^4.0.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "start": "run-p --print-name start:mock start:dev",
    "start:dev": "react-scripts start",
    "prestart:mock": "git checkout db.json",
    "start:mock": "json-server --watch db.json --delay 1000 --port 3001 --host 10.1.10.181",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "eqeqeq": [
        "error"
      ],
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".jsx"
          ]
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      },
      "react": {
        "version": "detect"
      }
    },
    "root": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
